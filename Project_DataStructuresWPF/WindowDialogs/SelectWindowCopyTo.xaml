<Window x:Class="Project_DataStructures.SelectWindowCopyTo"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Project_DataStructures"
        mc:Ignorable="d"
        Title="SelectWindowCopyTo" Height="116" Width="388.333">
    <Grid x:Name="grid">
        <CheckBox x:Name="checkBox" Content="Add to new tab" HorizontalAlignment="Left" VerticalAlignment="Top" IsChecked="True" Checked="checkBox_Checked" Unchecked="checkBox_Unchecked"/>
        <ComboBox x:Name="tablesComboBox" HorizontalAlignment="Left" Margin="150,0,0,0" VerticalAlignment="Top" Width="117" IsEnabled="False"/>

    </Grid>
</Window>
